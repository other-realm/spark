{
	"editor.wordWrap": "on",
	"python.autoComplete.extraPaths": [
		"~/micromamba/envs/sparkignite",
		"~/micromamba/envs/pkgs"
	],
	"python.analysis.extraPaths": [
		"~\\.vscode\\extensions\\roipoussiere.cadquery-0.1.3\\stubs",
		"~/micromamba/envs/sparkignite",
		"~/micromamba/envs/pkgs"
	],
	"python.defaultInterpreterPath": "~/micromamba/envs/sparkignite\\python.exe",
	"python.linting.pycodestyleEnabled": false,
	"python.linting.enabled": false,
	"python.analysis.autoImportCompletions": true,
	"python.linting.pylintArgs": [
		"--init-hook",
		"import sys; sys.path.append('~/micromamba/envs/sparkignite')",
		"--init-hook",
		"import sys; sys.path.append('~/micromamba/envs/pkgs')"
	],
	"python.terminal.activateEnvironment": true,
	"matlab-formatter.pythonPath": "~/micromamba/envs/sparkignite\\",
	"matlab-interactive-terminal.pythonPath": "~/micromamba/envs/sparkignite/",
	"cmake.configureOnOpen": true,
	"files.autoSave": "afterDelay",
	"python.analysis.typeCheckingMode": "off",
	"jupyter.widgetScriptSources": [
		"jsdelivr.com",
		"unpkg.com"
	],
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true,
		"*.stl":true,
		"*.step":true,
		"*.STEP":true,
		"*.exe":true,
		"*/.history/**":true
	  },
	"MicroPython.executeButton": [
		{
			"text": "â–¶",
			"tooltip": "Run",
			"alignment": "left",
